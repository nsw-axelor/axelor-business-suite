<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="production" package="com.axelor.apps.production.db" />

	<entity name="Sop" lang="java">

		<many-to-one name="productCategory" ref="com.axelor.apps.base.db.ProductCategory" title="Category" />
		<one-to-many name="sopLineList" ref="com.axelor.apps.production.db.SopLine" title="Sales and Operations Planning Lines" />
		<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
		<many-to-one name="year" ref="com.axelor.apps.base.db.Year" title="Year" />
		<boolean name="isForecastOnHistoric" title="Forecast on historic"/>
		<boolean name="isGenerated" default="false"/>
	</entity>

</domain-models>